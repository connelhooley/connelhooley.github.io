<% it.languages?.forEach(language => { %>
  <a class="tag" href="/blog/languages/<%= encodeURIComponent(language.replace('#', 'Sharp')) %>">
    <i class="fa-solid fw fa-code"></i>&nbsp;<%= language %>
  </a>
<% }); %>
<% it.technologies?.forEach(technology => { // TODO put this is page store %>
  <a class="tag" href="/blog/technologies/<%= encodeURIComponent(technology.replace('#', 'Sharp')) %>">
    <i class="fa-solid fw fa-tag"></i>&nbsp;<%= technology %>
  </a>
<% }); %>